<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" width="600" height="400" viewBox="0 0 600 400">
  <!-- Fondo con gradiente suave -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f5f5f5;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e0e0e0;stop-opacity:1" />
    </linearGradient>
    
    <!-- Patrón de textura para mantel -->
    <pattern id="mantelPattern" patternUnits="userSpaceOnUse" width="20" height="20" patternTransform="rotate(45)">
      <rect width="20" height="20" fill="#ffffff" />
      <rect width="10" height="10" fill="#f9f9f9" />
      <rect x="10" y="10" width="10" height="10" fill="#f9f9f9" />
    </pattern>
  </defs>
  
  <!-- Fondo -->
  <rect width="600" height="400" fill="url(#bgGradient)" />
  
  <!-- Mantel -->
  <rect x="100" y="150" width="400" height="200" rx="5" ry="5" fill="url(#mantelPattern)" stroke="#dddddd" stroke-width="2" />
  
  <!-- Plato principal -->
  <ellipse cx="300" cy="250" rx="150" ry="50" fill="#ffffff" stroke="#dddddd" stroke-width="2" />
  
  <!-- Tacos en el plato -->
  <!-- Taco 1 (Carne Asada) -->
  <g transform="translate(-50, 0) scale(0.8)">
    <path d="M250,180 C250,230 350,230 350,180 L350,230 C350,250 325,265 300,265 C275,265 250,250 250,230 L250,180 Z" fill="#F2D680" stroke="#E6C666" stroke-width="2" />
    
    <!-- Carne -->
    <path d="M270,190 L330,190 L325,210 L275,210 Z" fill="#8B4513" stroke="#5D2906" stroke-width="1" />
    
    <!-- Líneas de la parrilla -->
    <line x1="275" y1="195" x2="325" y2="195" stroke="#5D2906" stroke-width="1" />
    <line x1="275" y1="200" x2="325" y2="200" stroke="#5D2906" stroke-width="1" />
    <line x1="275" y1="205" x2="325" y2="205" stroke="#5D2906" stroke-width="1" />
    
    <!-- Cebolla -->
    <path d="M280,185 C285,180 295,180 300,185 C305,180 315,180 320,185 C315,190 305,190 300,185 C295,190 285,190 280,185 Z" fill="#FFFFFF" opacity="0.9" />
    
    <!-- Cilantro -->
    <g fill="#4CAF50">
      <path d="M290,180 C295,175 300,178 298,183 C293,185 288,183 290,180 Z" />
      <path d="M310,180 C315,175 320,178 318,183 C313,185 308,183 310,180 Z" />
    </g>
  </g>
  
  <!-- Taco 2 (Pollo) -->
  <g transform="translate(50, 0) scale(0.8)">
    <path d="M250,180 C250,230 350,230 350,180 L350,230 C350,250 325,265 300,265 C275,265 250,250 250,230 L250,180 Z" fill="#F2D680" stroke="#E6C666" stroke-width="2" />
    
    <!-- Pollo -->
    <path d="M270,190 L330,190 L325,210 L275,210 Z" fill="#F5DEB3" stroke="#DEB887" stroke-width="1" />
    
    <!-- Guacamole -->
    <path d="M280,185 C290,175 310,175 320,185 C310,195 290,195 280,185 Z" fill="#4CAF50" opacity="0.9" />
    
    <!-- Pico de gallo -->
    <g>
      <circle cx="290" cy="180" r="3" fill="#FF6347" opacity="0.9" />
      <circle cx="300" cy="178" r="3" fill="#FF6347" opacity="0.9" />
      <circle cx="310" cy="180" r="3" fill="#FF6347" opacity="0.9" />
      <path d="M295,182 C300,177 305,182 300,187 C295,182 300,177 295,182 Z" fill="#4CAF50" opacity="0.9" />
    </g>
  </g>
  
  <!-- Papas fritas -->
  <g transform="translate(-120, 20)">
    <path d="M200,230 L220,200 L240,230" fill="#FFD700" stroke="#E6C666" stroke-width="1" />
    <path d="M210,230 L230,195 L250,230" fill="#FFD700" stroke="#E6C666" stroke-width="1" />
    <path d="M220,230 L240,205 L260,230" fill="#FFD700" stroke="#E6C666" stroke-width="1" />
    <!-- Sal -->
    <circle cx="225" cy="215" r="1" fill="#FFFFFF" />
    <circle cx="235" cy="210" r="1" fill="#FFFFFF" />
    <circle cx="245" cy="220" r="1" fill="#FFFFFF" />
  </g>
  
  <!-- Refresco -->
  <g transform="translate(120, 0)">
    <rect x="350" y="200" width="30" height="50" rx="2" ry="2" fill="#000000" opacity="0.9" />
    <ellipse cx="365" cy="200" rx="15" ry="5" fill="#000000" opacity="0.9" />
    <ellipse cx="365" cy="200" rx="12" ry="3" fill="#333333" opacity="0.9" />
    <!-- Burbujas -->
    <circle cx="360" cy="210" r="1" fill="#FFFFFF" opacity="0.7" />
    <circle cx="370" cy="215" r="1" fill="#FFFFFF" opacity="0.7" />
    <circle cx="365" cy="220" r="1" fill="#FFFFFF" opacity="0.7" />
    <circle cx="358" cy="225" r="1" fill="#FFFFFF" opacity="0.7" />
    <circle cx="372" cy="230" r="1" fill="#FFFFFF" opacity="0.7" />
    <!-- Pajita -->
    <path d="M365,195 L375,175" stroke="#FF0000" stroke-width="2" />
    <path d="M375,175 L380,175" stroke="#FF0000" stroke-width="2" />
  </g>
  
  <!-- Salsa en recipiente pequeño -->
  <g transform="translate(0, 50)">
    <ellipse cx="300" cy="270" rx="30" ry="10" fill="#ffffff" stroke="#dddddd" stroke-width="1" />
    <ellipse cx="300" cy="265" rx="20" ry="5" fill="#D32F2F" opacity="0.8" />
  </g>
  
  <!-- Sombras para dar profundidad -->
  <ellipse cx="300" cy="320" rx="180" ry="20" fill="#000000" opacity="0.1" />
  
  <!-- Etiqueta de descuento -->
  <g transform="translate(450, 100) rotate(15)">
    <rect x="0" y="0" width="80" height="80" rx="40" ry="40" fill="#D72638" />
    <text x="40" y="35" font-family="Montserrat, sans-serif" font-size="24" font-weight="bold" text-anchor="middle" fill="#FFFFFF">15%</text>
    <text x="40" y="55" font-family="Montserrat, sans-serif" font-size="14" text-anchor="middle" fill="#FFFFFF">OFF</text>
  </g>
  
  <!-- Texto del producto -->
  <text x="300" y="50" font-family="Montserrat, sans-serif" font-size="28" font-weight="bold" text-anchor="middle" fill="#000000">Combo Individual</text>
  <text x="300" y="80" font-family="Open Sans, sans-serif" font-size="18" text-anchor="middle" fill="#666666">2 tacos + papas + refresco</text>
  <text x="300" y="110" font-family="Montserrat, sans-serif" font-size="22" font-weight="bold" text-anchor="middle" fill="#D72638">¡OFERTA ESPECIAL!</text>
</svg>