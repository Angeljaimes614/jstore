<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" width="1920" height="1080" viewBox="0 0 1920 1080">
  <!-- Fondo con gradiente -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2C3E50;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#000000;stop-opacity:1" />
    </linearGradient>
    
    <!-- Patr칩n de tacos para el fondo -->
    <pattern id="tacoPattern" x="0" y="0" width="300" height="300" patternUnits="userSpaceOnUse">
      <!-- Taco 1 -->
      <path d="M50,100 C50,150 150,150 150,100 L150,150 C150,170 130,190 100,190 C70,190 50,170 50,150 L50,100 Z" fill="#FFD700" opacity="0.2" transform="rotate(15, 100, 100)"/>
      
      <!-- Taco 2 -->
      <path d="M200,200 C200,250 300,250 300,200 L300,250 C300,270 280,290 250,290 C220,290 200,270 200,250 L200,200 Z" fill="#FFD700" opacity="0.2" transform="rotate(-20, 250, 250)"/>
    </pattern>
  </defs>
  
  <!-- Fondo principal -->
  <rect width="1920" height="1080" fill="url(#bgGradient)" />
  
  <!-- Patr칩n de tacos de fondo -->
  <rect width="1920" height="1080" fill="url(#tacoPattern)" />
  
  <!-- Overlay para oscurecer y dar contraste al texto -->
