    <path d="M-100,0 C-100,100 100,100 100,0 L100,100 C100,150 50,200 0,200 C-50,200 -100,150 -100,100 L-100,0 Z" fill="#FFD700" opacity="0.5"/>
    <path d="M-80,20 C-80,70 80,70 80,20" stroke="#D72638" stroke-width="5" fill="none" opacity="0.6"/>
