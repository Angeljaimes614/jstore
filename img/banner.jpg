    <!-- Ingredientes del taco -->
    <path d="M-80,20 C-80,70 80,70 80,20" stroke="#D72638" stroke-width="5" fill="none" opacity="0.9"/>
    <path d="M-60,50 C-60,90 60,90 60,50" stroke="#D72638" stroke-width="5" fill="none" opacity="0.9"/>
    
    <!-- Detalles adicionales -->
    <circle cx="-50" cy="30" r="10" fill="#4CAF50" opacity="0.8"/>
    <circle cx="0" cy="40" r="12" fill="#4CAF50" opacity="0.8"/>
    <circle cx="50" cy="25" r="8" fill="#4CAF50" opacity="0.8"/>
    
    <circle cx="-30" cy="60" r="7" fill="#FF5722" opacity="0.8"/>
    <circle cx="20" cy="70" r="9" fill="#FF5722" opacity="0.8"/>
    <circle cx="60" cy="65" r="6" fill="#FF5722" opacity="0.8"/>
  </g>
  
  <!-- Tacos adicionales m치s peque침os -->
