    <path d="M320,215 C330,205 340,210 335,220 C325,230 315,225 320,215 Z" />
    <path d="M380,220 C390,210 400,215 395,225 C385,235 375,230 380,220 Z" />
  </g>
  
  <!-- Ajo (pequeños trozos) -->
  <g fill="#FFFFFF" stroke="#EEEEEE" stroke-width="1">
