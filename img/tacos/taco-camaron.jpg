    <circle cx="220" cy="150" r="8" fill="none" stroke="#FFFFFF" stroke-width="1" />
    <circle cx="220" cy="150" r="5" fill="none" stroke="#FFFFFF" stroke-width="1" />
    <line x1="215" y1="145" x2="225" y2="155" stroke="#FFFFFF" stroke-width="1" />
    <line x1="215" y1="155" x2="225" y2="145" stroke="#FFFFFF" stroke-width="1" />
    
    <circle cx="380" cy="150" r="10" fill="#FFFF00" opacity="0.8" stroke="#E6E600" stroke-width="1" />
    <circle cx="380" cy="150" r="8" fill="none" stroke="#FFFFFF" stroke-width="1" />
    <circle cx="380" cy="150" r="5" fill="none" stroke="#FFFFFF" stroke-width="1" />
    <line x1="375" y1="145" x2="385" y2="155" stroke="#FFFFFF" stroke-width="1" />
    <line x1="375" y1="155" x2="385" y2="145" stroke="#FFFFFF" stroke-width="1" />
  </g>
  
  <!-- Cilantro -->
  <g fill="#4CAF50">
    <path d="M240,160 C245,155 250,158 248,163 C253,160 258,163 255,168 C250,170 245,168 240,160 Z" />
    <path d="M300,155 C305,150 310,153 308,158 C313,155 318,158 315,163 C310,165 305,163 300,155 Z" />
    <path d="M360,160 C365,155 370,158 368,163 C373,160 378,163 375,168 C370,170 365,168 360,160 Z" />
  </g>
  
  <!-- Sombras para dar profundidad -->
  <ellipse cx="300" cy="350" rx="180" ry="20" fill="#000000" opacity="0.1" />
  
  <!-- Texto del producto -->
  <text x="300" y="50" font-family="Montserrat, sans-serif" font-size="24" font-weight="bold" text-anchor="middle" fill="#000000">Taco de Camarón</text>
  <text x="300" y="80" font-family="Open Sans, sans-serif" font-size="16" text-anchor="middle" fill="#666666">Con aguacate y salsa picante</text>
</svg>