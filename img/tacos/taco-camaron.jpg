    <path d="M220,200 C230,190 240,195 235,205 C225,215 215,210 220,200 Z" fill="#FF6F61" stroke="#E55A4F" stroke-width="1" />
    <path d="M235,205 C240,200 245,205 240,210 C235,215 230,210 235,205 Z" fill="#FF6F61" stroke="#E55A4F" stroke-width="1" />
    <line x1="225" y1="200" x2="230" y2="210" stroke="#FFFFFF" stroke-width="1" />
    
    <!-- Camarón 2 -->
    <path d="M260,195 C270,185 280,190 275,200 C265,210 255,205 260,195 Z" fill="#FF6F61" stroke="#E55A4F" stroke-width="1" />
