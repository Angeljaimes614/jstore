    <path d="M235,175 L245,180" stroke="#388E3C" stroke-width="1" />
    <path d="M295,170 L305,175" stroke="#388E3C" stroke-width="1" />
    <path d="M355,175 L365,180" stroke="#388E3C" stroke-width="1" />
  </g>
  
  <!-- Salsa picante (gotas) -->
  <g fill="#D32F2F" opacity="0.8">
    <circle cx="250" cy="190" r="4" />
    <circle cx="280" cy="185" r="5" />
    <circle cx="310" cy="180" r="4" />
    <circle cx="340" cy="185" r="5" />
    <circle cx="370" cy="190" r="4" />
