    <!-- Camarón 3 -->
    <path d="M300,190 C310,180 320,185 315,195 C305,205 295,200 300,190 Z" fill="#FF6F61" stroke="#E55A4F" stroke-width="1" />
    <path d="M315,195 C320,190 325,195 320,200 C315,205 310,200 315,195 Z" fill="#FF6F61" stroke="#E55A4F" stroke-width="1" />
    <line x1="305" y1="190" x2="310" y2="200" stroke="#FFFFFF" stroke-width="1" />
    
    <!-- Camarón 4 -->
    <path d="M340,195 C350,185 360,190 355,200 C345,210 335,205 340,195 Z" fill="#FF6F61" stroke="#E55A4F" stroke-width="1" />
    <path d="M355,200 C360,195 365,200 360,205 C355,210 350,205 355,200 Z" fill="#FF6F61" stroke="#E55A4F" stroke-width="1" />
    <line x1="345" y1="195" x2="350" y2="205" stroke="#FFFFFF" stroke-width="1" />
    
    <!-- Camarón 5 -->
    <path d="M380,200 C390,190 400,195 395,205 C385,215 375,210 380,200 Z" fill="#FF6F61" stroke="#E55A4F" stroke-width="1" />
    <path d="M395,205 C400,200 405,205 400,210 C395,215 390,210 395,205 Z" fill="#FF6F61" stroke="#E55A4F" stroke-width="1" />
    <line x1="385" y1="200" x2="390" y2="210" stroke="#FFFFFF" stroke-width="1" />
  </g>
  
  <!-- Aguacate -->
  <g>
    <path d="M230,170 C240,160 260,165 255,180 C245,190 225,185 230,170 Z" fill="#4CAF50" opacity="0.9" />
    <path d="M290,165 C300,155 320,160 315,175 C305,185 285,180 290,165 Z" fill="#4CAF50" opacity="0.9" />
    <path d="M350,170 C360,160 380,165 375,180 C365,190 345,185 350,170 Z" fill="#4CAF50" opacity="0.9" />
    
