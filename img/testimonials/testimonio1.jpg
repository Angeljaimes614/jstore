<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" width="300" height="300" viewBox="0 0 300 300">
  <!-- Fondo con gradiente suave -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f5f5f5;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e0e0e0;stop-opacity:1" />
    </linearGradient>
    
    <!-- Patrón de textura para fondo -->
    <pattern id="texturePattern" patternUnits="userSpaceOnUse" width="20" height="20" patternTransform="rotate(45)">
      <rect width="20" height="20" fill="#ffffff" />
      <rect width="10" height="10" fill="#f9f9f9" />
      <rect x="10" y="10" width="10" height="10" fill="#f9f9f9" />
    </pattern>
  </defs>
  
  <!-- Fondo -->
  <rect width="300" height="300" fill="url(#bgGradient)" />
  
  <!-- Marco circular -->
  <circle cx="150" cy="150" r="140" fill="url(#texturePattern)" stroke="#FFD700" stroke-width="3" />
  
  <!-- Silueta de persona -->
  <g transform="translate(75, 60)">
    <!-- Cabeza -->
    <circle cx="75" cy="60" r="50" fill="#A0522D" />
    
    <!-- Cara -->
    <circle cx="75" cy="60" r="45" fill="#D2B48C" />
    
    <!-- Ojos -->
    <ellipse cx="60" cy="50" rx="5" ry="7" fill="#FFFFFF" />
    <circle cx="60" cy="50" r="3" fill="#000000" />
    <ellipse cx="90" cy="50" rx="5" ry="7" fill="#FFFFFF" />
    <circle cx="90" cy="50" r="3" fill="#000000" />
    
    <!-- Cejas -->
    <path d="M55,40 C60,35 70,38 65,42" stroke="#5D4037" stroke-width="2" fill="none" />
    <path d="M95,40 C90,35 80,38 85,42" stroke="#5D4037" stroke-width="2" fill="none" />
    
    <!-- Nariz -->
    <path d="M75,55 C78,65 72,65 75,70" stroke="#A0522D" stroke-width="2" fill="none" />
    
    <!-- Boca sonriente -->
    <path d="M60,80 C70,90 80,90 90,80" stroke="#A0522D" stroke-width="2" fill="none" />
    
    <!-- Cabello -->
    <path d="M30,40 C40,10 110,10 120,40 C125,20 115,5 75,5 C35,5 25,20 30,40 Z" fill="#5D4037" />
    
    <!-- Cuello -->
    <path d="M65,105 L85,105 L85,120 L65,120 Z" fill="#D2B48C" />
    
    <!-- Hombros -->
    <path d="M45,120 L105,120 L115,140 L35,140 Z" fill="#D72638" />
  </g>
  
  <!-- Estrellas de calificación -->
  <g transform="translate(75, 220)">
    <polygon points="0,0 5,15 20,15 8,25 13,40 0,30 -13,40 -8,25 -20,15 -5,15" fill="#FFD700" />
    <polygon points="40,0 45,15 60,15 48,25 53,40 40,30 27,40 32,25 20,15 35,15" fill="#FFD700" />
    <polygon points="80,0 85,15 100,15 88,25 93,40 80,30 67,40 72,25 60,15 75,15" fill="#FFD700" />
    <polygon points="120,0 125,15 140,15 128,25 133,40 120,30 107,40 112,25 100,15 115,15" fill="#FFD700" />
    <polygon points="160,0 165,15 180,15 168,25 173,40 160,30 147,40 152,25 140,15 155,15" fill="#FFD700" />
  </g>
  
  <!-- Comillas decorativas -->
  <text x="50" y="50" font-family="Georgia, serif" font-size="60" fill="#FFD700" opacity="0.3">"</text>
  <text x="230" y="200" font-family="Georgia, serif" font-size="60" fill="#FFD700" opacity="0.3">"</text>
</svg>